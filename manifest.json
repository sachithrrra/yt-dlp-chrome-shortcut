{
  "manifest_version": 3,
  "name": "yt-dlp-chrome-shortcut",
  "version": "1.0",
  "description": "Adds download copy link to yt-dlp",
  "permissions": ["activeTab"],
  "host_permissions": ["https://*.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
